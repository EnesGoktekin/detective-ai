{
  "functions": {
    "backend/[...path].mjs": {
      "runtime": "nodejs20.x",
      "memory": 512,
      "maxDuration": 10,
      "includeFiles": [
        "backend/db/gameData.js"
      ],
      "bodyParser": false 
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "backend/[...path].mjs" }
  ]
}
